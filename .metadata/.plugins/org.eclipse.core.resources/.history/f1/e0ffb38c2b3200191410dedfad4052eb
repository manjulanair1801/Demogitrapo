package parallel.execution;

import org.testng.annotations.AfterMethod;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.Test;

public class Class2 {
	
	@BeforeMethod
	public void beforeMethod2() {
		System.out.println("This is beforemethod2--class2");
		
	}
	
	@Test
	public void test2() {
		System.out.println("This is my second test --class2");
	}

	@AfterMethod
	public void AfterMethod2() {
		System.out.println("This is aftermethod2--class2");
		
	}
}


